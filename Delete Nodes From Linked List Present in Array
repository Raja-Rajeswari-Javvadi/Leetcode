class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

class Solution:
    def modifiedList(self, nums: list[int], head: ListNode | None) -> ListNode | None:
        dummy = ListNode(0, head)
        nums_set = set(nums)
        
        curr = dummy
        while curr.next:
            if curr.next.val in nums_set:
                curr.next = curr.next.next
            else:
                curr = curr.next
                
        return dummy.next
