class Solution:
    def numOfUnplacedFruits(self, fruits, baskets):
        n = len(fruits)
        used = [False] * n  # To track used baskets
        unplaced = 0

        for fruit in fruits:  # For each fruit
            placed = False
            for j in range(n):  # Look for suitable basket
                if not used[j] and baskets[j] >= fruit:
                    used[j] = True  # Mark basket as used only if fruit is placed
                    placed = True
                    break
            if not placed:
                unplaced += 1

        return unplaced
