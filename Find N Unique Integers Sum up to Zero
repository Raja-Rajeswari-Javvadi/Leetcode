class Solution {
public:
    std::vector<int> sumZero(int n) {
        std::vector<int> answer;
        if (n % 2 == 1) {
            answer.push_back(0);
        }
        for (int i = 1; i <= n / 2; ++i) {
            answer.push_back(i);
            answer.push_back(-i);
        }
        return answer;
    }
};
